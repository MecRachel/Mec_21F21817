class Car {
    int carID;
    String model, brand;
    
    void input(int id, String m, String b) {
        carID = id;
        model = m;
        brand = b;
    }
    
    void display() {
        System.out.println("Car ID: " + carID);
        System.out.println("Model: " + model);
        System.out.println("Brand: " + brand);
    }
}

class ElectricCar extends Car {
    int batteryCapacity;
    String manufacturer;
}
